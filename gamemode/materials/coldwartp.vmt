"UnlitGeneric"
{
	"$basetexture" "coldwartp"

	// light in the dark 
  	$selfillum 1
    	$selfillummask "coldwartp"

                     "Proxies"
             		  {
   	   	"AnimatedTexture"
    		  {
     		    "animatedtexturevar" "$basetexture"
      		    "animatedtextureframenumvar" "$frame"
         		    "animatedtextureframerate" "12"
     		  }
  	 }
	
}